      <Button layoutX="27.0" layoutY="319.0" mnemonicParsing="false" onAction="#onPosizioneDiUnaStellaRispettoSpinaDorsale" prefHeight="26.0" prefWidth="298.0" text="Posizione stella rispetto alla spina dorsale" AnchorPane.leftAnchor="27.0" AnchorPane.topAnchor="319.0" />
      <Button fx:id="inFileBtn" layoutX="444.0" layoutY="65.0" mnemonicParsing="false" onAction="#onInsertFilePressed" text="Inserisci File" />
      <Button fx:id="searchFil" layoutX="22.0" layoutY="162.0" mnemonicParsing="false" onAction="#searchFil" text="Ricerca Filamenti e informazione Segmenti" />
      <Label layoutX="30.0" layoutY="60.0" text="Ricerca Filamenti:" />
      <Label layoutX="24.0" layoutY="212.0" text="Ricerca Stelle:" />
   </children>
</AnchorPane>
