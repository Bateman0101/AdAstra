<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.HomePageBoundary">
   <children>
      <Label layoutX="237.0" layoutY="22.0" prefHeight="21.0" prefWidth="89.0" text="Ad Astra">
         <font>
            <Font name="System Bold Italic" size="17.0" />
         </font>
      </Label>
      <Button fx:id="bSat" layoutX="423.0" layoutY="139.0" mnemonicParsing="false" onAction="#onSatellitePressed" prefHeight="26.0" prefWidth="146.0" text="Inserisci Satellite" />
      <Button fx:id="bStrum" layoutX="422.0" layoutY="180.0" mnemonicParsing="false" onAction="#onStrumentoPressed" prefHeight="26.0" prefWidth="146.0" text="Inserisci Strumento" />
      <Button fx:id="button" layoutX="22.0" layoutY="93.0" mnemonicParsing="false" onAction="#onInfoPressed" prefHeight="26.0" prefWidth="298.0" text="Informazioni Filamenti" AnchorPane.leftAnchor="22.0" AnchorPane.topAnchor="93.0" />
      <Button layoutX="22.0" layoutY="126.0" mnemonicParsing="false" onAction="#onRegionPressed" prefHeight="26.0" prefWidth="298.0" text="Ricerca Filamenti in una regione" AnchorPane.leftAnchor="22.0" AnchorPane.topAnchor="126.0" />
      <Button layoutX="25.0" layoutY="245.0" mnemonicParsing="false" onAction="#onStelleInFilPressed" prefHeight="26.0" prefWidth="298.0" text="Ricerca stelle interne ad un Filamento" AnchorPane.leftAnchor="25.0" AnchorPane.topAnchor="245.0" />
      <Button fx:id="bUtente" layoutX="407.0" layoutY="221.0" mnemonicParsing="false" onAction="#onUtentePressed" prefHeight="26.0" prefWidth="176.0" text="Inserisci nuovo utente" />
