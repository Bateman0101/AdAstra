      <TextField fx:id="idEntry" layoutX="23.0" layoutY="80.0" prefHeight="26.0" prefWidth="137.0" promptText="Inserisci ID" />
      <TextField fx:id="satelliteEntry" layoutX="200.0" layoutY="80.0" prefHeight="26.0" prefWidth="137.0" promptText="Inserisci Satellite" />
      <Label layoutX="32.0" layoutY="182.0" text="Posizione del centroide:" />
