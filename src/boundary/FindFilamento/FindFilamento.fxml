      <TextField fx:id="latEntry" layoutX="315.0" layoutY="175.0" prefHeight="26.0" prefWidth="55.0" />
      <TextField fx:id="lonEntry" layoutX="510.0" layoutY="176.0" prefHeight="26.0" prefWidth="55.0" />
      <Label layoutX="37.0" layoutY="250.0" prefHeight="16.0" prefWidth="145.0" text="Estensione:" />
